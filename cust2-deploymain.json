{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "deploychoice": {
            "type": "string",
            "metadata": {
                "description": "Environment to Deploy"
            },
          "allowedValues": [
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/nmarket-template-multisvrdeploy_withkey.cse.dsc.nodomain.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/evh-template-domaincontrollers_withkey.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-s2s.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-apptier.twosvrdeploy.json"
          ]
        },
        "paramchoice": {
            "type": "string",
            "metadata": {
                "description": "Different Parameters files choice"
            },
          "allowedValues": [
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/nmarket-parameters-key.svrsnodomainscentral-03.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/evh-parameters-dr.key.domaincontrollers1.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-dr.s2s.parameters.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-webtier.parameters.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-apptier.appsvrs.parameters.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-apptier.edwvrs.parameters.json",
            "https://raw.githubusercontent.com/boklyn/Cust2Testing/master/cust2-apptier.rulessvrs.parameters.json"
          ]
        }

    },
    "resources": [
        {
            "apiVersion": "2015-01-01",
            "name": "ProductionDeployment",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[parameters('deploychoice')]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[parameters('paramchoice')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ]
}